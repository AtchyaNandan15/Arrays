#include <iostream>

using namespace std;

int main()
{
  

  int myArray[5];
  int i;
  
  for(i=0;i<5;i++) {
      cout<<"Enter a name";
      cin>>myArray[i];
      
  }
  
  for(i=0;i<5;i++) {
      
      cout<<myArray[i]<<"\n";
  }
  
  
}
 
